


<h4 class="text-2xl mt-32 md:mt-40">Why not check out another project?</h4>
<div class="flex flex-wrap" x-data="{ tags: ['personal'] }">
  {% for project in projects %}
    <div class="h-48 w-48 p-2 flex-grow-0 flex items-stretch" data-tags={{ project.tags }}>
      <div class="w-full bg-blue-100 relative">
      {% if project.thumb %}
        {% capture src %}
          {{project.thumb}}
        {% endcapture %}
        <a href={{project.path}} class="block w-full h-full img">
          {% img src "object-cover object-center h-full" %}
        </a>
      {% endif %}
      </div>
    </div>
  {% endfor %}
</div>